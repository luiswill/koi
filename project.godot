; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Koi",
"language": "GDScript",
"path": "res://scripts/Koi.gd"
}, {
"base": "Node",
"class": "Plant",
"language": "GDScript",
"path": "res://scripts/Plant.gd"
}, {
"base": "Node",
"class": "Pond",
"language": "GDScript",
"path": "res://scripts/pond.gd"
}, {
"base": "Node",
"class": "User",
"language": "GDScript",
"path": "res://scripts/user.gd"
} ]
_global_script_class_icons={
"Koi": "",
"Plant": "",
"Pond": "",
"User": ""
}

[application]

config/name="Koi"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

GLOBAL="*res://scripts/global.gd"
USER="*res://scripts/user.gd"
KOI="*res://scripts/Koi.gd"
POND="*res://scripts/pond.gd"
GAME="*res://scripts/gameSettings.gd"
DB="*res://scripts/database.gd"
DBMODEL="*res://scripts/databaseModel.gd"
EVENTS="*res://scripts/Events.gd"
GESTURES="*res://assets/godot-gesture/GestureDetector.gd"

[display]

window/size/width=400
window/size/height=700
window/size/resizable=false
window/handheld/orientation="portrait"

[editor_plugins]

enabled=PoolStringArray( "godot-sqlite" )

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
environment/default_environment="res://default_env.tres"
